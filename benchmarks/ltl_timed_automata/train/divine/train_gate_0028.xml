<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
    <declaration>
    const int a=2;
    const int b=5;
    const int c=1;
    const int d=2;
    const int e=1;
    typedef int[1, 28] id_t;
    chan exit_c;
    chan lower;
    chan raise;
    chan approach;
    </declaration>
    <template>
        <name>Train</name>
        <parameter>const id_t id</parameter>
        <declaration>clock x;</declaration>
        <location id="id0">
            <name>train0</name>
        </location>
        <location id="id1">
            <name>train1</name>
            <label kind="invariant">x &lt;= b</label>
        </location>
        <location id="id2">
            <name>train2</name>
            <label kind="invariant">x &lt;= b</label>
        </location>
        <location id="id3">
            <name>train3</name>
            <label kind="invariant">x &lt;= b</label>
        </location>
        <init ref="id0"/>
        <transition>
            <source ref="id2"/>
            <target ref="id3"/>
        </transition>
        <transition>
            <source ref="id3"/>
            <target ref="id0"/>
            <label kind="guard">x &lt;= b</label>
            <label kind="synchronisation">exit_c?</label>
        </transition>
        <transition>
            <source ref="id1"/>
            <target ref="id2"/>
            <label kind="guard">x &gt; a</label>
        </transition>
        <transition>
            <source ref="id0"/>
            <target ref="id1"/>
            <label kind="synchronisation">approach?</label>
            <label kind="assignment">x := 0</label>
        </transition>
    </template>
    <template>
        <name>Gate</name>
        <declaration>clock y;</declaration>
        <location id="id4">
            <name>gate0</name>
        </location>
        <location id="id5">
            <name>gate1</name>
            <label kind="invariant">y &lt;= c</label>
        </location>
        <location id="id6">
            <name>gate2</name>
        </location>
        <location id="id7">
            <name>gate3</name>
            <label kind="invariant">y &lt;= d</label>
        </location>
        <init ref="id4"/>
        <transition>
            <source ref="id5"/>
            <target ref="id6"/>
            <label kind="guard">y &lt;= c</label>
        </transition>
        <transition>
            <source ref="id6"/>
            <target ref="id7"/>
            <label kind="synchronisation">raise?</label>
            <label kind="assignment">y := 0</label>
        </transition>
        <transition>
            <source ref="id4"/>
            <target ref="id5"/>
            <label kind="synchronisation">lower?</label>
            <label kind="assignment">y := 0</label>
        </transition>
        <transition>
            <source ref="id7"/>
            <target ref="id4"/>
            <label kind="guard">y &gt;= c &amp;&amp; y &lt;= d</label>
        </transition>
    </template>
    <template>
        <name>Controller</name>
        <declaration>clock z; int cnt;</declaration>
        <location id="id8">
            <name>controller0</name>
        </location>
        <location id="id9">
            <name>controller1</name>
            <label kind="invariant">z &lt;= e</label>
        </location>
        <location id="id10">
            <name>controller2</name>
        </location>
        <location id="id11">
            <name>controller3</name>
            <label kind="invariant">z &lt;= e</label>
        </location>
        <init ref="id8"/>
        <transition>
            <source ref="id8"/>
            <target ref="id9"/>
            <label kind="synchronisation">approach!</label>
            <label kind="assignment">z := 0, cnt := 1</label>
        </transition>
        <transition>
            <source ref="id9"/>
            <target ref="id9"/>
            <label kind="synchronisation">approach!</label>
            <label kind="assignment">cnt := cnt + 1</label>
        </transition>
        <transition>
            <source ref="id9"/>
            <target ref="id9"/>
            <label kind="synchronisation">exit_c!</label>
            <label kind="assignment">cnt := cnt - 1</label>
        </transition>
        <transition>
            <source ref="id10"/>
            <target ref="id10"/>
            <label kind="synchronisation">approach!</label>
            <label kind="assignment">cnt := cnt + 1</label>
        </transition>
        <transition>
            <source ref="id10"/>
            <target ref="id10"/>
            <label kind="guard">cnt &gt; 1</label>
            <label kind="synchronisation">exit_c!</label>
            <label kind="assignment">cnt := cnt - 1</label>
        </transition>
        <transition>
            <source ref="id10"/>
            <target ref="id11"/>
            <label kind="guard">cnt == 1</label>
            <label kind="synchronisation">exit_c!</label>
            <label kind="assignment">z := 0, cnt := 0</label>
        </transition>
        <transition>
            <source ref="id11"/>
            <target ref="id8"/>
            <label kind="guard">z &lt;= e</label>
            <label kind="synchronisation">raise!</label>
        </transition>
        <transition>
            <source ref="id11"/>
            <target ref="id10"/>
            <label kind="guard">z &lt;= e</label>
            <label kind="synchronisation">approach!</label>
            <label kind="assignment">cnt := cnt + 1</label>
        </transition>
        <transition>
            <source ref="id9"/>
            <target ref="id10"/>
            <label kind="guard">z == e</label>
            <label kind="synchronisation">lower!</label>
        </transition>
    </template>
    <system>
      train0 = Train(1);
      train1 = Train(2);
      train2 = Train(3);
      train3 = Train(4);
      train4 = Train(5);
      train5 = Train(6);
      train6 = Train(7);
      train7 = Train(8);
      train8 = Train(9);
      train9 = Train(10);
      train10 = Train(11);
      train11 = Train(12);
      train12 = Train(13);
      train13 = Train(14);
      train14 = Train(15);
      train15 = Train(16);
      train16 = Train(17);
      train17 = Train(18);
      train18 = Train(19);
      train19 = Train(20);
      train20 = Train(21);
      train21 = Train(22);
      train22 = Train(23);
      train23 = Train(24);
      train24 = Train(25);
      train25 = Train(26);
      train26 = Train(27);
      train27 = Train(28);

    gate = Gate();
    controller = Controller();
    system train0, train1, train2, train3, train4, train5, train6, train7, train8, train9, train10, train11, train12, train13, train14, train15, train16, train17, train18, train19, train20, train21, train22, train23, train24, train25, train26, train27, gate, controller;</system>
    <queries>
    </queries>
</nta>
